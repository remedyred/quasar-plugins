{
	"name": "@snickbit/quasar",
	"description": "My custom Quasar components. Use with caution.",
	"version": "0.1.45",
	"license": "MIT",
	"main": "./dist/snickbit-quasar.umd.js",
	"module": "./dist/snickbit-quasar.es.js",
	"exports": {
		".": {
			"import": "./dist/snickbit-quasar.es.js",
			"require": "./dist/snickbit-quasar.cjs.js"
		},
		"./dist/snickbit-quasar.css": {
			"import": "./dist/snickbit-quasar.css",
			"require": "./dist/snickbit-quasar.css"
		},
		"./dist/variables.scss": {
			"import": "./dist/variables.scss",
			"require": "./dist/variables.scss"
		}
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"index": "indexer src",
		"clean": "shx rm -rf dist",
		"watch": "vite build --watch",
		"build:styles": "bundle-scss -m ./src/styles/variabes.scss -d ./dist/variables.scss",
		"build:js": "vite build",
		"depcheck": "depcheck",
		"prod": "npm run index && npm run build:js && npm run build:styles"
	},
	"dependencies": {
		"@quasar/extras": "^1.13.5",
		"@snickbit/utilities": "^3.0.15",
		"mitt": "^3.0.0",
		"quasar": "^2.6.6",
		"vue": "^3.2.33",
		"vue-router": "^4.0.14"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.0.9",
		"@remedyred/indexer": "^2.0.68",
		"@vitejs/plugin-vue": "^2.3.1",
		"bundle-scss": "^1.5.1",
		"sass": "^1.50.0",
		"shx": "^0.3.4",
		"vite": "^2.9.5"
	}
}
