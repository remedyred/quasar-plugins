{
	"name": "@snickbit/quasar-forms",
	"description": "Quasar form generation",
	"version": "0.0.24",
	"license": "MIT",
	"main": "./dist/snickbit-quasar-forms.umd.js",
	"module": "./dist/snickbit-quasar-forms.es.js",
	"exports": {
		".": {
			"import": "./dist/snickbit-quasar-forms.es.js",
			"require": "./dist/snickbit-quasar-forms.umd.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"index": "indexer src",
		"clean": "shx rm -rf dist",
		"watch": "vite build --watch",
		"build": "vite build",
		"depcheck": "depcheck",
		"prod": "yarn index && vite build"
	},
	"dependencies": {
		"@snickbit/minia": "^0.0.15",
		"@snickbit/utilities": "^3.0.16",
		"quasar": "^2.6.6",
		"vue": "^3.2.33",
		"vue-router": "^4.0.14"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.0.9",
		"@remedyred/indexer": "^2.0.79",
		"@vitejs/plugin-vue": "^2.3.1",
		"sass": "1.51.0",
		"shx": "^0.3.4",
		"vite": "^2.9.5"
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/minia",
			"@snickbit/utilities"
		]
	}
}
