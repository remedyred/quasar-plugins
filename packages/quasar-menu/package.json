{
	"name": "@snickbit/quasar-menu",
	"description": "Quasar sidebar menu generation using @snickbit/minia",
	"version": "0.1.2",
	"license": "MIT",
	"main": "./dist/snickbit-quasar-menu.umd.js",
	"module": "./dist/snickbit-quasar-menu.es.js",
	"exports": {
		".": {
			"import": "./dist/snickbit-quasar-menu.es.js",
			"require": "./dist/snickbit-quasar-menu.umd.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"index": "indexer",
		"clean": "shx rm -rf dist",
		"watch": "vite build --watch --mode 'development",
		"build": "vite build",
		"depcheck": "depcheck",
		"prod": "npm run index && vite build"
	},
	"dependencies": {
		"@snickbit/minia": "^0.0.20",
		"@snickbit/utilities": "^3.0.17",
		"quasar": "^2.6.6",
		"vue": "^3.2.33",
		"vue-router": "^4.0.14"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.0.9",
		"@remedyred/indexer": "^2.2.6",
		"@vitejs/plugin-vue": "^2.3.1",
		"sass": "^1.51.0",
		"shx": "^0.3.4",
		"vite": "^2.9.5"
	},
	"nx": {
		"implicitDependencies": [
			"@snickbit/minia",
			"@snickbit/utilities"
		]
	}
}
