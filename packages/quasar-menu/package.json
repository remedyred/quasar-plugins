{
	"name": "@snickbit/quasar-menu",
	"version": "0.1.7",
	"description": "Quasar sidebar menu generation using @snickbit/minia",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/snickbit-quasar-menu.es.js",
			"require": "./dist/snickbit-quasar-menu.umd.js"
		}
	},
	"main": "./dist/snickbit-quasar-menu.umd.js",
	"module": "./dist/snickbit-quasar-menu.es.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"clean": "shx rm -rf dist",
		"depcheck": "depcheck",
		"index": "indexer",
		"prod": "pnpm run index && vite build",
		"watch": "vite build --watch --mode 'development"
	},
	"dependencies": {
		"@snickbit/minia": "workspace:^0.0.25",
		"@snickbit/utilities": "workspace:^3.0.23",
		"quasar": "^2.7.1",
		"vue": "^3.2.36",
		"vue-router": "^4.0.15"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.0.10",
		"@remedyred/indexer": "^2.4.1",
		"@vitejs/plugin-vue": "^2.3.3",
		"sass": "^1.52.1",
		"shx": "^0.3.4",
		"vite": "^2.9.9"
	}
}
