{
	"name": "@snickbit/quasar-menu",
	"version": "0.1.15",
	"description": "Quasar sidebar menu generation using @snickbit/minia",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/snickbit-quasar-menu.es.js",
			"require": "./dist/snickbit-quasar-menu.umd.js"
		}
	},
	"main": "./dist/snickbit-quasar-menu.umd.js",
	"module": "./dist/snickbit-quasar-menu.es.js",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"clean": "shx rm -rf dist",
		"depcheck": "depcheck",
		"index": "indexer",
		"prod": "pnpm run index && vite build",
		"watch": "vite build --watch --mode 'development"
	},
	"dependencies": {
		"@snickbit/minia": "^0.0.35",
		"@snickbit/utilities": "^3.2.0",
		"quasar": "^2.7.3",
		"vue": "^3.2.37",
		"vue-router": "^4.0.16"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "1.1.1",
		"@snickbit/indexer": "2.5.15",
		"@vitejs/plugin-vue": "3.1.0",
		"sass": "1.54.5",
		"shx": "0.3.4",
		"vite": "3.1.0",
		"vite-plugin-dts": "^1.4.1"
	}
}
